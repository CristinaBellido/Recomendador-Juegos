    "url = \"https://www.gsmarena.com/lava-phones-94.php\"\n",
    "href_list = []\n",
    "\n",
    "while True:\n",
    "    response = requests.get(url)\n",
    "    soup = BeautifulSoup(response.content, \"html.parser\")\n",
    "    \n",
    "    links = soup.find_all('a', href=True)\n",
    "\n",
    "    for link in links:\n",
    "        href_list.append(link['href'])\n",
    "\n",
    "    next_link = soup.find('a', {'class': 'pages-next'})\n",
    "    \n",
    "    if next_link:\n",
    "        next_url = next_link['href']\n",
    "        if not next_url.startswith('http'):\n",
    "            # Si el href no tiene un esquema, agregar el esquema correcto antes de hacer la solicitud\n",
    "            next_url = f\"https://www.gsmarena.com/{next_url}\"\n",
    "        url = next_url\n",
    "        time.sleep(4) # Esperar 4 segundos antes de continuar a la siguiente página\n",
    "    else:\n",
    "        break\n",
    "\n",
    "print(href_list)"
